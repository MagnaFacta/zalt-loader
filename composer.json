{
    "name": "MagnaFacta/zalt-loader",
    "description": "Zend Alternative Loader",
    "type": "library",
    "require": {
        "php": ">=5.6.0",
        "zendframework/zend-di": ">=2.6",
        "zendframework/zend-servicemanager": ">=3.1"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*"
    },
    "authors": [
        {
            "name": "Matijs de Jong",
            "email": "mjong@magnafacta.nl"
        },
        {
            "name": "Menno Dekker",
            "email": "menno.dekker@erasmusmc.nl"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Zalt\\Loader\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Zalt\\Loader\\": "test\\Zalt\\Loader",
            "Test1\\": "test\\Test1",
            "Test2\\": "test\\Test2",
            "Test3\\": "test\\Test3"
        }
    },
    "repositories": [
    ]
}
